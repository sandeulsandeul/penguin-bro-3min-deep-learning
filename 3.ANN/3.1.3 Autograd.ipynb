   "source": [
    "import torch\n",
    "w = torch.tensor(1.0,requires_grad = True) \n",
    "# requires_grad = True 이면 Autograd 계산 시 w 미분 값을 w.grad에 저장  \n",
    "a = w*2 \n",
    "l = a**2 + a \n",
    "l.backward()\n",
    "print('l을 w로 미분한 값은 {}'.format(w.grad))"
   ]
